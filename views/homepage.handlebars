  <div class="jumbotron">
      <h1>Welcome to Tech Blog!</h1>
      <p>A place where developers can share their thoughts and experiences about the world of web development and engineering.</p>
    </div>
  {{#each blogDataClear as |blog| }}
  <div class="post">
    <h2 class="post-title">{{blog.title}}</h2>
    <p class="post-content">{{blog.contents}}</p>
    <p>Created by: {{blog.user.name}}</p>
  </div>
{{/each}}


</div>
